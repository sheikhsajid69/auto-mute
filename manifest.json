{
  "manifest_version": 3,
  "name": "YouTube Ad Auto Mute",
  "version": "1.1",
  "description": "Automatically mutes YouTube ads within milliseconds",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "YouTube Ad Auto Mute"
  }
}